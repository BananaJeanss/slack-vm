{
    "display_information": {
        "name": "Slack VM",
        "description": "Control a QEMU VM, via slack",
        "background_color": "#001a69"
    },
    "features": {
        "bot_user": {
            "display_name": "Slack Plays",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "channels:history",
                "chat:write",
                "files:write",
                "groups:history",
                "reactions:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "message.channels",
                "message.groups"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}